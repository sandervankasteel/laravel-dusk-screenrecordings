{
    "name": "vstyler96/laravel-dusk-screenrecording",
    "description": "A package to have screen recordings of Dusk tests",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sandervankasteel\\LaravelDuskScreenrecordings\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sander van Kasteel",
            "email": "info@sandervankasteel.nl"
        },
        {
            "name": "Vincent GS",
            "email": "vstyler96@gmail.com"
        }
    ],
    "require": {
        "php": "~8.0|~8.1",
        "laravel/framework": "^8.83"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sandervankasteel\\LaravelDuskScreenrecordings\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/dusk": "^6.25",
        "phpunit/phpunit": "^9.6",
        "orchestra/testbench": "^6.25"
    }
}
